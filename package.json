{
  "name": "n8n-xtend",
  "version": "1.1.0",
  "description": "Extend your n8n experience - client-side enhancements for n8n",
  "author": "TBLgGamin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TBLgGamin/n8n-xtend.git"
  },
  "homepage": "https://github.com/TBLgGamin/n8n-xtend",
  "bugs": {
    "url": "https://github.com/TBLgGamin/n8n-xtend/issues"
  },
  "keywords": [
    "n8n",
    "browser-extension",
    "chrome-extension",
    "automation",
    "workflow",
    "tree-view"
  ],
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun run scripts/build.ts",
    "dev": "bun run scripts/build.ts --watch",
    "typecheck": "bun x tsc --noEmit",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/bun": "^1.1.14",
    "@types/chrome": "^0.0.287",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
