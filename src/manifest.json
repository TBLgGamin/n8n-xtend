{
  "manifest_version": 3,
  "name": "n8n-xtend",
  "description": "Extend your n8n experience with tree navigation, workflow capture, and more",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
